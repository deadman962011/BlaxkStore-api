extends layout


block content

 .container
   .col-sm-3
    .AddImageBtn(style='border:1px solid gray;height:320px; margin:6px ')
     form.form-inline( method="post")
      input.form-control(type="text", name="imgSrcI")
      input.btn.btn-primary(type="submit", value="add")
     
   if(images)
    each image in images
     .col-sm-3
      .AddImageBtn(style='border:1px solid gray; margin:6px ')
       img.img-responsive(src=""+image.item.imgSrc, alt="alt")
       
    